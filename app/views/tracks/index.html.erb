<p style="color: green"><%= notice %></p>

<h1>Tracks</h1>

<div id="tracks">
  <% @tracks.each_with_index do |track, i| %>
    <h2>Track: <%= (65 + i).chr %></h2>
    <%= render track %>
    <p>
      <%= link_to "Show this track", track %>
    </p>
  <% end %>
</div>

<%= link_to "New track", new_track_path %>
