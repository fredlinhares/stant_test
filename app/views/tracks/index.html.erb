<p style="color: green"><%= notice %></p>

<h1>Tracks</h1>

<div id="tracks">
  <% @tracks.each_with_index do |track, i| %>
    <h2>Track: <%= (65 + i).chr %></h2>
    <%= render track %>
    <p>
      <%= link_to "Show this track", track %>
    </p>
  <% end %>
</div>

<%= form_with url: tracks_load_file_path, method: :post do |form| %>
  <%= form.file_field :presentation_list %>
  <%= form.submit "Upload" %>
<% end %>

<%= link_to "New track", new_track_path %>
